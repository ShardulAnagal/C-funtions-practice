#include<stdio.h>
// Write a C program to find maximum and minimum between two numbers using functions.

int maxormin(int x,int y){
    if (x<y)
    {
        return 1;
    }
    else return 0;
}

void main(){
    int a, b;
    printf("enter two numbers\n");
    scanf("%d%d", &a, &b);
    int r= maxormin(a, b);
    if (r=1 && r!=0)
    {
        printf("%d is greater than %d", b, a);
    }
    else printf("%d is greater than %d", a, b);
} 
